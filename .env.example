# OpenAI API Key (required for AI features)
OPENAI_API_KEY=your_openai_api_key_here

# GitHub Personal Access Token (required for GitHub OAuth)
GITHUB_TOKEN=your_github_token_here

# Database URLs (required for registry)
DATABASE_URL_RAW=your_database_connection_string
DATABASE_URL=your_prisma_accelerate_url

# Pinecone Vector Database (required for job recommendations)
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment

# Supabase (required for some registry features)
SUPABASE_PUBLIC_KEY=your_supabase_public_key
SUPABASE_KEY=your_supabase_service_role_key

# Perplexity AI (optional, for enhanced AI features)
PERPLEXITY_API_KEY=your_perplexity_api_key
